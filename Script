local dragging
local dragInput
local dragStart
local startPos
local glitching = false
local reaching = false
local playerHasBall = false

-- Functions --

local function update(input)
	local delta = input.Position - dragStart
	UIHolder.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

local function getBall()
	local args = {
		[1] = "NoobGlitch"
	}

	game:GetService("ReplicatedStorage"):WaitForChild("Client"):FireServer(unpack(args))

	local args = {
		[1] = workspace:WaitForChild("TPS")
	}

	game:GetService("ReplicatedStorage"):WaitForChild("ChangeOwner"):FireServer(unpack(args))
end

local function checkForBall()
	local ball = workspace:FindFirstChild("TPS")
	if ball then
		if ball.Owner.Value == plr.Name or ball.Owner.Value == plr or ball.Owner.Value == plr.Character then
			bottomtext.Text = [[Ball is owned: ‚úÖ]]
			return ball
		else
			getBall()
			bottomtext.Text = [[Ball is owned: ‚ùå]]
		end
	end
end

local function glitchBallFunction()
	local ball = checkForBall()
	if ball and glitching then
		ball.Position = Vector3.new(0,30,0)
		local bodyForce = ball:FindFirstChild("BodyForce")
		if bodyForce then
			bodyForce = Vector3.new(0,1000,0)
			ball.Velocity = Vector3.new(0,0,0)
		end
	end
end

local function reachingFunction()
	if reaching then
		getBall()
	end
end

local function bringBallFunction()
	local ball = checkForBall()
	if ball then
		local char = plr.Character
		if char then
			local bodyForce = ball:FindFirstChild("BodyForce")
			if bodyForce then
				bodyForce = Vector3.new(0,0,0)
				ball.Velocity = Vector3.new(0,0,0)
			end
			ball.CFrame = plr.Character.PrimaryPart.CFrame * CFrame.new(0,0,-5)
		end
	end
end

local function scoreGoalFunction()
	local ball = checkForBall()
	if ball then
		if plr.Team == game.Teams["Home"] or plr.Team == game.Teams["-Home GK"] then
			ball.Position = workspace.WorkspaceStadiumMap.Goal22.Position
		else
			ball.Position = workspace.WorkspaceStadiumMap.Goal11.Position
		end
	end
end

local function createNewOption(parent,text,isButton)
	local frame = Instance.new("Frame",parent)
	frame.BackgroundColor3 = Color3.fromRGB(25,25,25)
	frame.Size = UDim2.new(1,0,0,30)
	frame.BorderSizePixel = 0
	local uipadding = Instance.new("UIPadding",frame)
	uipadding.PaddingTop = UDim.new(0,6)
	uipadding.PaddingLeft = UDim.new(0,6)
	uipadding.PaddingRight = UDim.new(0,6)
	uipadding.PaddingBottom = UDim.new(0,6)
	local textLabel = Instance.new("TextLabel",frame)
	textLabel.BackgroundTransparency = 1
	textLabel.Font = Enum.Font.SourceSansLight
	textLabel.Text = text
	textLabel.TextColor3 = Color3.fromRGB(255,255,255)
	textLabel.TextScaled = true
	textLabel.Size = UDim2.new(1,0,1,0)
	textLabel.TextXAlignment = Enum.TextXAlignment.Left
	if isButton then
		local frame = Instance.new("TextButton",frame)
		frame.AutoButtonColor = false
		frame.AnchorPoint = Vector2.new(1,0)
		frame.BackgroundColor3 = Color3.fromRGB(50,50,50)
		frame.Position = UDim2.new(1,0,0,0)
		frame.Size = UDim2.new(0,36,1,0)
		frame.BorderSizePixel = 0
		frame.Text = "Run"
		frame.TextScaled = true
		frame.TextColor3 = Color3.fromRGB(255,255,255)
		frame.Font = Enum.Font.SourceSans
		return frame
	else
		local frame = Instance.new("Frame",frame)
		frame.AnchorPoint = Vector2.new(1,0)
		frame.BackgroundColor3 = Color3.fromRGB(50,50,50)
		frame.Position = UDim2.new(1,0,0,0)
		frame.Size = UDim2.new(0,36,1,0)
		frame.BorderSizePixel = 0
		local frame = Instance.new("ImageButton",frame)
		frame.Size = UDim2.new(1,0,1,0)
		frame.BorderSizePixel = 0
		frame.BackgroundColor3 = Color3.fromRGB(255,255,255)
		frame.AutoButtonColor = false
		local aspectRatio = Instance.new("UIAspectRatioConstraint",frame)
		local enabled = false
		frame.MouseButton1Down:Connect(function()
			local speed = 0.2
			if enabled then
				switch.Pitch = 0.5
				switch:Play()
				tweenService:Create(frame,TweenInfo.new(speed),{AnchorPoint = Vector2.new(0,0)}):Play()
				tweenService:Create(frame,TweenInfo.new(speed),{Position = UDim2.new(0,0,0,0)}):Play()
			else
				switch.Pitch = 1
				switch:Play()
				tweenService:Create(frame,TweenInfo.new(speed),{AnchorPoint = Vector2.new(1,0)}):Play()
				tweenService:Create(frame,TweenInfo.new(speed),{Position = UDim2.new(1,0,0,0)}):Play()
			end
			enabled = not enabled
		end)
		return frame
	end
end

-- Script --

infoButton.MouseButton1Down:Connect(function()
	controlTab.Visible = false
	silentTab.Visible = false
	infoTab.Visible = true
	controlButton.BackgroundTransparency = 1
	silentButton.BackgroundTransparency = 1
	infoButton.BackgroundTransparency = 0
end)

silentButton.MouseButton1Down:Connect(function()
	controlTab.Visible = false
	silentTab.Visible = true
	infoTab.Visible = false
	controlButton.BackgroundTransparency = 1
	silentButton.BackgroundTransparency = 0
	infoButton.BackgroundTransparency = 1
end)

controlButton.MouseButton1Down:Connect(function()
	controlTab.Visible = true
	silentTab.Visible = false
	infoTab.Visible = false
	controlButton.BackgroundTransparency = 0
	silentButton.BackgroundTransparency = 1
	infoButton.BackgroundTransparency = 1
end)

local scoreGoal = createNewOption(controlTab,"Score a Goal",true)
local bringBall = createNewOption(controlTab,"Bring the Ball",true)
local TPBall = createNewOption(controlTab,"TP to the Ball",true)
local hideBall = createNewOption(controlTab,"Hide the Ball",false)
local autoScore = createNewOption(controlTab,"Goal Farm Mode",false)

local autoReach = createNewOption(silentTab,"AutoReach",false)
local autoAim = createNewOption(silentTab,"AutoAim (WIP)",false)
local autoReach = createNewOption(silentTab,"AutoCatch (WIP)",false)

TopBar.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true

		dragStart = input.Position
		startPos = UIHolder.Position

		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

TopBar.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		dragInput = input
	end
end)

game:GetService("UserInputService").InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		update(input)
	end
end)

exitButton.MouseButton1Down:Connect(function()
	UI:Destroy()
end)

hideBall.MouseButton1Down:Connect(function()
	glitching = not glitching
end)

bringBall.MouseButton1Down:Connect(function()
	getBall()
	bringBallFunction()
end)

autoReach.MouseButton1Down:Connect(function()
	reaching = not reaching
end)

scoreGoal.MouseButton1Down:Connect(function()
	getBall()
	scoreGoalFunction()
end)

game["Run Service"].RenderStepped:Connect(function()
	checkForBall()
	reachingFunction()
	glitchBallFunction()
end)
